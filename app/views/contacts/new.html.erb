<div class="row">
  <div class="col-md-12 text-center">
    <h1>Adicionar Novo Contato</h1>
  </div>

  <div class="col-md-8 col-md-offset-2 col-xs-12">
    <%= form_for  @contact do |f| %>
      <%= render 'form', f: f %>

      <%= f.submit 'Salvar', class: 'btn btn-primary mb-2' %>
    <% end %>
  </div>
</div>